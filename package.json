{
    "name": "@perryjohnson/extjs-pkg-mantis",
    "description": "ExtJS Package Wrapper for Mantis BugTracker",
    "version": "1.4.2",
    "license": "MIT",
    "author": {
        "name": "Scott Meesseman"
    },
    "bugs": {
        "url": "https://app1.development.pjats.com/projects/set_project.php?project=extjs-pkg-mantis&make_default=no&ref=bug_report_page.php"
    },
    "homepage": "https://app1.development.pjats.com/svn/web/listing.php?repname=pja&path=%2Fextjs-pkg-mantis%2Ftrunk%2F",
    "repository": {
        "type": "svn",
        "url": "https://svn.development.pjats.com/pja/extjs-pkg-mantis/trunk"
    },
    "devDependencies": {},
    "displayName": "ExtJS Package Wrapper for Mantis",
    "files": [
        "classic",
        "modern",
        "src",
        "sass"
    ],
    "keywords": [
        "sencha extjs",
        "mantis",
        "ticket system",
        "project management"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "app-publish": "app-publisher -p ps",
        "app-publish-dry": "app-publisher -p ps --dry-run"
    },
    "sencha": {
        "name": "mantis",
        "namespace": "Ext.ux",
        "framework": "ext",
        "type": "code",
        "creator": "spmeesseman",
        "summary": "Package wrapper singleton for mantis",
        "detailedDescription": "Package wrapper singleton for mantis RPC",
        "version": "1.0.0.0",
        "compatversion": "1.0.0",
        "format": "1",
        "slicer": null,
        "output": "${framework.dir}/build/packages/${package.name}",
        "local": true,
        "resource": {
            "paths": ""
        },
        "resources": [
            {
              "path": "${package.dir}/resources",
              "output": "shared"
            }
        ],
        "classic": {
            "sass": {
                "namespace": "Ext.ux",
                "etc": [
                    "${package.dir}/sass/etc"
                ],
                "var": [
                    "${package.dir}/sass/var"
                ],
                "src": [
                    "${package.dir}/sass/src",
                    "${package.dir}/classic/sass/src"
                ]
            }
        },
        "modern": {
            "sass": {
                "namespace": "Ext.ux",
                "etc": [
                    "${package.dir}/sass/etc"
                ],
                "var": [
                    "${package.dir}/sass/var"
                ],
                "src": [
                    "${package.dir}/sass/src",
                    "${package.dir}/modern/sass/src"
                ]
            }
        },
        "classpath": [
            "${package.dir}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],
        "requires": [
            "core"
        ],
        "ignore": [
            "(^|/)CVS(/?$|/.*?$)"
        ]
    }
}
