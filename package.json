{
    "name": "@spmeesseman/extjs-pkg-mantis",
    "description": "ExtJS Package Wrapper for Mantis RPC",
    "version": "1.0.2",
    "license": "MIT",
    "author": {
        "name": "Scott Meesseman",
        "email": "spmeesseman@gmail.com"
    },
    "homepage": "https://github.com/spmeesseman/extjs-pkg-mantis",
    "repository": {
        "type": "git",
        "url": "https://github.com/spmeesseman/pja/extjs-pkg-mantis.git"
    },
    "bugs": {
        "url": "https://github.com/spmeesseman/extjs-pkg-mantis/issues"
    },
    "devDependencies": {
        "@perryjohnson/app-publisher": "^1.4.13"
    },
    "displayName": "ExtJS Package Wrapper for Mantis",
    "files": [
        "classic",
        "modern",
        "src",
        "sass"
    ],
    "keywords": [
        "sencha extjs",
        "mantis",
        "ticket system",
        "project management"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "app-publish": "app-publisher -p ps",
        "app-publish-dry": "app-publisher -p ps --dry-run"
    },
    "sencha": {
        "name": "mantis",
        "namespace": "Ext.ux",
        "framework": "ext",
        "type": "code",
        "creator": "spmeesseman",
        "summary": "Package wrapper singleton for mantis",
        "detailedDescription": "Package wrapper singleton for mantis RPC",
        "version": "1.0.0.0",
        "compatversion": "1.0.0",
        "format": "1",
        "slicer": null,
        "output": "${framework.dir}/build/packages/${package.name}",
        "local": true,
        "resource": {
            "paths": ""
        },
        "classic": {
            "sass": {
                "namespace": "Ext.ux",
                "etc": [
                    "${package.dir}/sass/etc"
                ],
                "var": [
                    "${package.dir}/sass/var"
                ],
                "src": [
                    "${package.dir}/sass/src",
                    "${package.dir}/classic/sass/src"
                ]
            }
        },
        "modern": {
            "sass": {
                "namespace": "Ext.ux",
                "etc": [
                    "${package.dir}/sass/etc"
                ],
                "var": [
                    "${package.dir}/sass/var"
                ],
                "src": [
                    "${package.dir}/sass/src",
                    "${package.dir}/modern/sass/src"
                ]
            }
        },
        "classpath": [
            "${package.dir}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],
        "requires": [
            "core"
        ],
        "ignore": [
            "(^|/)CVS(/?$|/.*?$)"
        ]
    }
}
